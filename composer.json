{
    "name": "rebelcode/rcmod-wp-bookings-cqrs",
    "description": "A RebelCode module that provides the WordPress CQRS resource models for RebelCode booking plugins.",
    "type": "rc-module",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "ext-mysqli": "^0.1",
        "rebelcode/modular": "^0.2",
        "rebelcode/sql-cqrs-resource-models-abstract": "^0.2-alpha1",
        "rebelcode/wp-cqrs-resource-models": "^0.2-alpha1",
        "dhii/data-object-abstract": "^0.1-alpha1",
        "dhii/placeholder-template": "^0.1-alpha2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "10up/wp_mock": "^0.1",
        "mikey179/vfsStream": "^1.6.0",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "rebelcode/module-testing": "dev-task/initial-code",
        "dhii/output-renderer-interface": "^0.3-alpha2",
        "johnpbloch/wordpress-core": "^4.0",
        "rebelcode/rcmod-wp-cqrs": "dev-task/add-tests"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\Storage\\Resource\\WordPress\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RebelCode\\Storage\\UnitTest\\Resource\\WordPress\\": "test/unit",
            "RebelCode\\Storage\\FuncTest\\Resource\\WordPress\\": "test/functional"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    }
}
